   72 func_hdr: FUNCTION IDENT
   73         | FUNCTION FUNCREF

   74 $@13: /* empty */

   75 $@14: /* empty */

   76 func_def: func_hdr $@13 func_args $@14 statement_list END

   77 func_args: /* empty */

   78 $@15: /* empty */

   79 func_args: '(' $@15 func_arg_list ')'

   80 func_arg_list: IDENT
   81              | func_arg_list ',' IDENT

   82 decl_type: INT
