Grammar

    0 $accept: script $end

    1 script: statement_list

    2 statement_list: /* empty */
    3               | statement_list statement stmnt_terminator

    4 stmnt_terminator: '\n'
    5                 | ';'

    6 statement: while_stmnt
    7          | for_stmnt
    8          | foreach_stmnt
    9          | if_stmnt
   10          | assign_stmnt
   11          | include_stmnt
   12          | endscript_marker
   13          | cmd_stmnt
   14          | func_call
   15          | func_def
   16          | ext_func
   17          | decl_stmnt
   18          | break_stmnt
   19          | return_stmnt
   20          | null_stmnt

   21 endscript_marker: ENDSCRIPT

   22 $@1: /* empty */

   23 while_stmnt: WHILE '(' expr ')' $@1 statement_list END

   24 $@2: /* empty */

   25 for_stmnt: FOR '(' assign_stmnt ';' expr ';' assign_stmnt ')' $@2 statement_list END

   26 $@3: /* empty */

   27 $@4: /* empty */

   28 foreach_stmnt: FOREACH VARREF '(' $@3 ws arg_list ws $@4 ')' statement_list END

   29 $@5: /* empty */

   30 if_stmnt: IF '(' expr ')' $@5 statement_list else_clause END

   31 else_clause: /* empty */

   32 $@6: /* empty */

   33 else_clause: ELSE $@6 statement_list

   34 assign_stmnt: VARREF '=' expr

   35 $@7: /* empty */

   36 include_hdr: INCLUDE $@7 ws LITERAL

   37 include_stmnt: include_hdr WHITESPACE arg_list ws
   38              | include_hdr ws

   39 opt_node: /* empty */
   40         | LITERAL

   41 $@8: /* empty */

   42 cmd_name: IDENT $@8 opt_node

   43 $@9: /* empty */

   44 cmd_stmnt: cmd_name $@9 opt_arg_list ws
   45          | cmd_name '(' optwslist func_call_list optwslist ')'

   46 funcref: FUNCREF

   47 func_call: funcref '(' optwslist func_call_list optwslist ')'

   48 $@10: /* empty */

   49 func_call: funcref $@10 opt_arg_list ws

   50 opt_arg_list: /* empty */
   51             | opt_arg_list wslist arg_component_list

   52 arg_list: arg_component_list
   53         | arg_list wslist arg_component_list

   54 optwslist: /* empty */
   55          | wslist

   56 wslist: WHITESPACE
   57       | wslist WHITESPACE

   58 func_call_list: arg_component_list
   59               | func_call_list optwslist ',' optwslist arg_component_list

   60 ws: /* empty */
   61   | WHITESPACE

   62 arg_component_list: arg_component
