   64 arg_component: LITERAL
   65              | STRCONST
   66              | DOLLARARG

   67 $@11: /* empty */

   68 $@12: /* empty */

   69 arg_component: '{' $@11 expr $@12 '}'

   70 ext_func: EXTERN IDENT
