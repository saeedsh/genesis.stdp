   23 endscript_marker: ENDSCRIPT

   24 @2: /* empty */

   25 while_stmnt: WHILE '(' expr ')' @2 statement_list END

   26 @3: /* empty */

   27 for_stmnt: FOR '(' assign_stmnt ';' expr ';' assign_stmnt ')' @3 statement_list END

   28 $@4: /* empty */

   29 $@5: /* empty */

   30 foreach_stmnt: FOREACH VARREF '(' $@4 optwslist arg_list optwslist $@5 ')' statement_list END

   31 @6: /* empty */

   32 if_stmnt: IF '(' expr ')' @6 statement_list else_clause END

   33 else_clause: /* empty */
   34            | ELSE statement_list

   35 @7: /* empty */

   36 else_clause: ELIF '(' expr ')' @7 statement_list else_clause

   37 assign_stmnt: VARREF '=' expr

   38 $@8: /* empty */

   39 include_hdr: INCLUDE $@8 ws

   40 include_stmnt: include_hdr LITERAL WHITESPACE arg_list ws
